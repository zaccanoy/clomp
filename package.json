{
  "name": "clomp",
  "version": "1.0.2",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "A class name composer that helps you style your components with utility styles. It's like styled-components for CSS utility frameworks.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "yarn workspace @clomp/core build; yarn workspace @clomp/react build",
    "test": "yarn workspace @clomp/core test; yarn workspace @clomp/react test",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*"
  },
  "bugs": {
    "url": "https://github.com/zaccanoy/clomp/issues",
    "email": "zaccanoy@gmail.com"
  },
  "keywords": [
    "css-in-js",
    "class name composition",
    "react",
    "tailwind",
    "css utility framework"
  ],
  "homepage": "https://github.com/zaccanoy/clomp",
  "repository": {
    "type": "git",
    "url": "https://github.com/zaccanoy/clomp.git"
  },
  "author": "Zac Canoy <zaccanoy@gmail.com> (zaccanoy.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/parser": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "babel-loader": "^9.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "prettier": "^2.8.8",
    "react-dom": "^16.14.0"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^16.14.0"
  },
  "packageManager": "yarn@3.5.1"
}
