{
  "name": "clomp",
  "version": "1.0.0",
  "description": "A class name composer that helps you style your components with utility styles. It's like styled-components for CSS utility frameworks.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "bugs": {
    "url": "https://github.com/zaccanoy/clomp/issues",
    "email": "zaccanoy@gmail.com"
  },
  "keywords": [
    "css-in-js",
    "class name composition",
    "react",
    "tailwind",
    "css utility framework"
  ],
  "homepage": "https://github.com/zaccanoy/clomp",
  "repository": {
    "type": "git",
    "url": "https://github.com/zaccanoy/clomp.git"
  },
  "author": "Zac Canoy <zaccanoy@gmail.com> (zaccanoy.com)",
  "private": false,
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/parser": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.5",
    "babel-jest": "^26.6.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.1",
    "jest-cli": "^26.6.1",
    "jest-enzyme": "^7.1.2",
    "prettier": "^2.1.2",
    "react-dom": "^16.14.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.14.0"
  }
